VECTOR_DIM=1536
GEMINI_API_KEY=change-me
GEMINI_MODEL_QA=gemini-2.5-pro
GEMINI_MODEL_EMBED=text-embedding-004
STORAGE_BUCKET_URL=https://storage.local/aksara
